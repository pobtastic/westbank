; Main game loop
;
; Used by the routine at #R$CAC8.
c$CAE7 LD A,$7F                             ; {Read from the keyboard;
 $CAE9 IN A,($FE)                           ; #TABLE(default,centre,centre,centre,centre,centre,centre)
                                            ; { =h,r2 Port Number | =h,c5 Bit }
                                            ; { =h 0 | =h 1 | =h 2 | =h 3 | =h 4 }
                                            ; { #N$7F | SPACE | FULL-STOP | M | N | B }
                                            ; TABLE#}
 $CAEB RRA                                  ; Was "SPACE" pressed?
 $CAEC JR C,$CAFB                           ; Jump forward if not.
 $CAEE LD A,$FE                             ; {Read from the keyboard;
 $CAF0 IN A,($FE)                           ; #TABLE(default,centre,centre,centre,centre,centre,centre)
                                            ; { =h,r2 Port Number | =h,c5 Bit }
                                            ; { =h 0 | =h 1 | =h 2 | =h 3 | =h 4 }
                                            ; { #N$FE | SHIFT | Z | X | C | V }
                                            ; TABLE#}
 $CAF2 RRA                                  ; Was "CAPS SHIFT" pressed?
 $CAF3 JR C,$CAFB                           ; Jump forward if not.
 $CAF5 LD HL,$CBB4                          ; { CAPS + SPACE pressed = BREAK. Abort the game.
 $CAF8 CALL $FD80                           ; }
; The game has not been aborted.
; Check if it is paused and loop round if so until it is unpaused.
*$CAFB LD A,$FE                             ; {Read from the keyboard;
 $CAFD IN A,($FE)                           ; #TABLE(default,centre,centre,centre,centre,centre,centre)
                                            ; { =h,r2 Port Number | =h,c5 Bit }
                                            ; { =h 0 | =h 1 | =h 2 | =h 3 | =h 4 }
                                            ; { #N$FE | SHIFT | Z | X | C | V }
                                            ; TABLE#}
 $CAFF BIT 1,A                              ; Was 'Z' pressed? (PAUSE)
 $CB01 JR NZ,$CB0B                          ; Jump forward if not.
*$CB03 LD A,$FE                             ; {Read from the keyboard;
 $CB05 IN A,($FE)                           ; #TABLE(default,centre,centre,centre,centre,centre,centre)
                                            ; { =h,r2 Port Number | =h,c5 Bit }
                                            ; { =h 0 | =h 1 | =h 2 | =h 3 | =h 4 }
                                            ; { #N$FE | SHIFT | Z | X | C | V }
                                            ; TABLE#}
 $CB07 BIT 2,A                              ; was 'X' pressed? (UNPAUSE)
 $CB09 JR NZ,$CB03                          ; Loop back round if not.
; The game is running.
*$CB0B CALL $D2CF                           ; Get the keyboard or joystick input from the player.
 $CB0E LD BC,($CE1C)                        ; { Get the game speed from #R$CE1C
*$CB12 DEC BC                               ; and delay for that amount of time.
 $CB13 LD A,B                               ;
 $CB14 OR C                                 ;
 $CB15 JR NZ,$CB12                          ; }
 $CB17 CALL $D411                           ; Check for closed doors and movement.
                                            ; This routine exits here if all doors are closed and all money is collected.
; At this point, the player is not moving and the phase is still in play,
; so look at the doors.
 $CB1A CALL $CBA6                           ; Clear the temporary storage from #R$5B00 to #N$5BFF.
; Look at door 1.
 $CB1D LD IX,$D165                          ; Point to the first door at #R$D165.
 $CB21 LD (IX+$08),$00                      ; Clear "deposit made" flag on this door.
 $CB25 LD HL,$CE14                          ; Check the contents of the first door at #R$CE14.
 $CB28 LD A,(HL)                            ; { If there no character at this door, the door will point to #N$0000,
 $CB29 INC HL                               ; and the zero flag will be set.
 $CB2A OR (HL)                              ; }
 $CB2B PUSH AF                              ; Remember the flags.
 $CB2C CALL Z,$CE21                         ; If the door is empty, look at putting a character in.
 $CB2F POP AF                               ; Restore the flags.
 $CB30 JR Z,$CB3F                           ; If the door was empty, skip the next bit.
; There's somebody at the door.
 $CB32 LD HL,($CE14)                        ; Get the contents of the first door.
 $CB35 LD DE,$CB3F                          ; { Remember a return to #R$CB3F on the stack.
 $CB38 PUSH DE                              ; }
 $CB39 LD A,$01                             ; { Looking at the first door.
 $CB3B LD ($CE1A),A                         ; }
 $CB3E JP (HL)                              ; Do the action.
; Look at door 2.
*$CB3F LD IX,$D173                          ; Point to the second door at #R$D173.
 $CB43 LD (IX+$08),$00                      ; Clear "deposit made" flag on this door.
 $CB47 LD HL,$CE16                          ; Check the contents of the second door at #R$CE16.
 $CB4A LD A,(HL)                            ; { If there no character at this door, the door will point to #N$0000,
 $CB4B INC HL                               ; and the zero flag will be set.
 $CB4C OR (HL)                              ; }
 $CB4D PUSH AF                              ; Remember the flags.
 $CB4E CALL Z,$CE21                         ; If the door is empty, look at putting a character in.
 $CB51 POP AF                               ; Restore the flags.
 $CB52 JR Z,$CB61                           ; If the door was empty, skip the next bit.
; There's somebody at the door.
 $CB54 LD HL,($CE16)                        ; Get the contents of the second door.
 $CB57 LD DE,$CB61                          ; { Remember a return to #R$CB61 on the stack.
 $CB5A PUSH DE                              ; }
 $CB5B LD A,$02                             ; { Looking at the second door.
 $CB5D LD ($CE1A),A                         ; }
 $CB60 JP (HL)                              ; Do the action.
; Look at door 3.
*$CB61 LD IX,$D181                          ; Point to the thrd door at #R$D181.
 $CB65 LD (IX+$08),$00                      ; Clear "deposit made" flag on this door.
 $CB69 LD HL,$CE18                          ; Check the contents of the thrd door at #R$CE18.
 $CB6C LD A,(HL)                            ; { If there no character at this door, the door will point to #N$0000,
 $CB6D INC HL                               ; and the zero flag will be set.
 $CB6E OR (HL)                              ; }
 $CB6F PUSH AF                              ; Remember the flags.
 $CB70 CALL Z,$CE21                         ; If the door is empty, look at putting a character in.
 $CB73 POP AF                               ; Restore the flags.
 $CB74 JR Z,$CB83                           ; If the door was empty, skip the next bit.
; There's somebody at the door.
 $CB76 LD HL,($CE18)                        ; Get the contents of the door.
 $CB79 LD DE,$CB83                          ; { Remember a return to #R$CB83 on the stack.
 $CB7C PUSH DE                              ; }
 $CB7D LD A,$03                             ; { Looking at the third door.
 $CB7F LD ($CE1A),A                         ; }
