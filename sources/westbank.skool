; Run the appropriate keyboard or joystick routine
;
; Used by the routines at #R$CAE7 and #R$D3EA.
@label=Controls
c$D2CF LD HL,($CE12) ; #R$CE12
 $D2D2 LD DE,$D2D7   ; {Stash #R$D2D7 on the stack. Control returns there
 $D2D5 PUSH DE       ; after the keyboard or joystick is read.}
 $D2D6 JP (HL)       ; Jumps to the control routine for whatever is in-use
                     ; (either #R$D392 or #R$D39B).
 $D2D7 DEFW $AF4F    ;

; Take action on player input
;
; This routine is called indirectly from #R$D2CF
;
; A Bits set, depending on user input
c$D2D7 LD C,A        ;
 $D2D8 XOR A         ;
 $D2D9 LD HL,$D2FF   ;
 $D2DC LD (HL),A     ;
 $D2DD INC HL        ;
 $D2DE LD (HL),A     ;
 $D2DF INC HL        ;
 $D2E0 LD (HL),A     ;
 $D2E1 INC HL        ;
 $D2E2 LD (HL),A     ;
 $D2E3 LD A,C        ;
 $D2E4 BIT 4,A       ;
 $D2E6 JR NZ,$D307   ;
 $D2E8 LD HL,$D303   ;
 $D2EB LD (HL),$00   ;
 $D2ED INC HL        ;
 $D2EE LD (HL),$00   ;
 $D2F0 INC HL        ;
 $D2F1 LD (HL),$00   ;
 $D2F3 BIT 0,A       ;
 $D2F5 JP NZ,$D386   ;
 $D2F8 BIT 1,A       ;
 $D2FA JP NZ,$D38C   ;
 $D2FD RET           ;

