*$C8F0 CALL $C959    ; Change from night back to day.
 $C8F3 CALL $CA64    ; New day (day 2).
 $C8F6 LD B,$07      ; Seven daytime phases on day 2, all with every character.
*$C8F8 PUSH BC       ; Remember this
 $C8F9 CALL $CA81    ; Run this phase.
 $C8FC POP BC        ; Restore phase counter (so stack is correct for next instruction).
 $C8FD RET C         ; Return if the game's over.
; Phase 11 - 16
 $C8FE PUSH BC       ; Remember counter again.
 $C8FF CALL $C941    ; Speed the game up.
 $C902 POP BC        ; Restore phase counter again.
 $C903 DJNZ $C8F8    ; Loop back if there are more phases to run.
; Phase 17
 $C905 CALL $C95E    ; Change from day to dusk.
 $C908 CALL $CA81    ; Run this phase.
 $C90B RET C         ; Return if the game's over.
; Phase 18
 $C90C CALL $C963    ; Change from dusk to night.
 $C90F CALL $CA81    ; Run this phase.
 $C912 RET C         ; Return if the game's over.
; Phase 19 and beyond
 $C913 LD HL,$D191   ; {Having got through all the phases,
 $C916 LD A,(HL)     ; are we on day 2?
 $C917 LD (HL),$00   ; Also reset the flag so the reward screen isn't shown for day 3 and above.
 $C919 AND A         ; }
 $C91A JR Z,$C8F0    ; Jump back if we're not
 $C91C LD HL,$CBB4   ; We have completed two days, display the reward screen.
 $C91F LD DE,$5B80   ;
 $C922 LD BC,$0040   ;
 $C925 LDIR          ;
 $C927 CALL $C720    ; Display the reward screen.
 $C92A CALL $CD64    ; { Wait for #N$400 cycles 
 $C92D CALL $CD64    ;
 $C930 CALL $CD64    ;
 $C933 CALL $CD64    ; }
*$C936 XOR A         ; { Wait for a key to be pressed.
 $C937 IN A,($FE)    ; Read the keyboard to bits 0-4 ...
 $C939 OR $E0        ; ... and turn on bits 5-7.
 $C93B INC A         ; If no key is pressed, this will set A to 0.
 $C93C JR Z,$C936    ; Loop round if this is the case. }
 $C93E NOP           ;
 $C93F JR $C8F0      ; Loop back for another day.

; Make the game run faster
;
; Used by the routine at #R$C7E5.
;
; .
;
; This routine is called at the start of each phase, and makes it faster
;       than than the previous by decreasing the wait time.
;       This starts at #N$500 (slowest) and is decreased by #N$40 for each call,
;       so things generally open faster.
c$C941 PUSH AF       ; { Remember some registers.
 $C942 PUSH HL       ; }
 $C943 LD HL,($CE1C) ; Get the current speed.
 $C946 LD BC,$0040   ; { Subtract #N$40 from it.
 $C949 AND A         ; (The AND A clears the carry flag so SBC gives the desired result).
 $C94A SBC HL,BC     ; }
 $C94C LD A,H        ; { Is the speed now zero?
 $C94D OR L          ; }
 $C94E JR NZ,$C953   ; Jump forward if not.
 $C950 LD HL,$0040   ; The fastest speed is #N$40.
*$C953 LD ($CE1C),HL ; Set this as the new speed.
 $C956 POP HL        ; { Restore registers.
 $C957 POP AF        ; }
 $C958 RET           ;

