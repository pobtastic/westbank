 $C5AE LD A,(IX+$0B)   ; { Redraw the dueller as drawing and return.
 $C5B1 CALL $C661      ;
 $C5B4 RET             ; }
; State 2
*$C5B5 DEC (IX+$10)    ; Decrease the time left in this state.
 $C5B8 RET NZ          ; Return if not there yet.
 $C5B9 INC (IX+$09)    ; Move to the next state.
 $C5BC RET             ;
; State 3
*$C5BD LD HL,$C5F6     ; {If the player shot this dueller, call #R$C5F6 and exit.
 $C5C0 CALL $C5EB      ; Nice one!}
 $C5C3 DEC (IX+$02)    ; Decrease the time left in this state.
 $C5C6 RET NZ          ; Return if not there yet.
 $C5C7 LD (IX+$08),A   ; Reset the "dueller standing" flag
 $C5CA INC A           ; { Set the "player shot" flag
 $C5CB LD ($C6E2),A    ; which will cause a BANG!
 $C5CE RET             ; then return }
; State 4
*$C5CF DEC (IX+$0E)    ; Decrease the time left in this state.
 $C5D2 RET NZ          ; Return if not there yet.
 $C5D3 INC (IX+$09)    ; Move state between draw and shot.
 $C5D6 LD A,(IX+$0C)   ; { Redraw the dueller as shot and return.
 $C5D9 CALL $C661      ;
 $C5DC RET             ; }
; State 5
*$C5DD DEC (IX+$0F)    ; Decrease the time left in this state.
 $C5E0 RET NZ          ; Return if not there yet.
 $C5E1 LD A,$02        ; { Redraw the dueller as "floor"
 $C5E3 CALL $C661      ; All characters use the same sprite ID #N$02.}
 $C5E6 LD (IX+$08),$00 ; {Reset the "dueller still standing" flag and return.
 $C5EA RET             ; }

