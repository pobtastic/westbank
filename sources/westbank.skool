@label=CashboxReference_Inactive
g$D45C DEFB $01      ; Holds 01-12 which refer to the deposit cash boxes
                     ; (#R$CC8D onwards).
@label=CashboxReference_Active
 $D45D DEFB $01      ; Holds 01-12 which refer to the deposit cash boxes
                     ; (#R$CC8D onwards).

; Highlight Active Doors
;
; Used by the routines at #R$C9B7 and #R$D3EA.
;
; .
;
; This routine highlights the doors which are currently being displayed in the
; playarea.
@label=ActiveDoors
c$D45E LD A,($D45C)  ; #REGa=#R$D45C.
 $D461 LD C,$38      ; #REGc=#N$38 (not highlighted).
 $D463 CALL $D46B    ; Call #R$D46B.
 $D466 LD A,($D45D)  ; #REGa=#R$D45D.
 $D469 LD C,$3A      ; #REGc=#N$3A (highlighted).
; Sets a counter as three doors are highlighted as being "in-view".
@label=ActiveDoors_Start
*$D46B LD B,$03      ; #REGb=#N$03.
@label=ActiveDoors_Loop
*$D46D PUSH AF       ; Stash #REGaf on the stack.
 $D46E CALL $D47E    ; Call #R$D47E.
 $D471 POP AF        ; Restore #REGaf from the stack.
 $D472 INC A         ; Increment #REGa by one.
 $D473 CP $0D        ; {If #REGa is #N$0D then call #R$D47B.
 $D475 CALL NC,$D47B ; }
 $D478 DJNZ $D46D    ; Decrease the #REGb counter by one and loop back to
                     ; #R$D46D until it is zero.
 $D47A RET           ; Return.
; If the active doors spill over the last door, then cycle back to the beginning.
;       For example, if door 11 is active, then it's 11, 12 and 1 which are highlighted.
@label=ActiveDoors_SetDoorOne
*$D47B LD A,$01      ; #REGa=#N$01.
 $D47D RET           ; Return.
; Handles writing the attribute value held by #REGc to the screen.
@label=ActiveDoors_Paint
*$D47E CP $07        ; Is #REGa less than #N$07?
 $D480 LD HL,$5805   ; #REGhl=#N$5805 (left-hand side).
 $D483 JR NC,$D488   ; If #REGa was less than #N$07, jump to #R$D488.
 $D485 LD HL,$58FF   ; #REGhl=#N$58FF (right-hand side).
@label=ActiveDoors_Paint_Skip
*$D488 ADD A,A       ; {#REGhl=#REGhl + #REGa * #N$02.
 $D489 ADD A,L       ;
 $D48A LD L,A        ; }
 $D48B LD (HL),C     ; {Writes the attribute value held by #REGc to #REGhl and #REGhl + #N$01.
 $D48C INC HL        ;           This is because the number images are two characters wide.
 $D48D LD (HL),C     ; }
 $D48E RET           ; Return.

